@page
@model RazorEventMaker25InClass.Pages.Events.CreateEventModel
@{
    ViewData["Title"] = "Create event";
}
<h1>Create event</h1>
<form method="post">
    @* Alt der skal sendes tilbage skal lige i en form *@
    <div  class="form-group">
        <label asp-for="@Model.NewEvent.Name" ></label>
        <input asp-for="@Model.NewEvent.Name" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="@Model.NewEvent.Description"></label>
        <input asp-for="@Model.NewEvent.Description" class="form-control" />
    </div>
    <div  class="form-group">
        <label asp-for="@Model.NewEvent.City" ></label>
        <input asp-for="@Model.NewEvent.City" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="@Model.NewEvent.DateTime"></label>
        <input asp-for="@Model.NewEvent.DateTime" class="form-control" />
    </div>

</form>